# This is the default .env file
# It will be overridden by .env.development or .env.production based on the environment

# Server Configuration
PORT=5001
NODE_ENV=development

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/resq

# Security
JWT_SECRET=your-development-jwt-secret-key
ENCRYPTION_KEY=your-32-char-dev-encryption-key!!!

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Default Settings
LOG_LEVEL=debug
ENABLE_REQUEST_LOGGING=true
ENABLE_SWAGGER=true
RATE_LIMIT=false

# Token Configuration
ACCESS_TOKEN_EXPIRY=1d
REFRESH_TOKEN_EXPIRY=7d 